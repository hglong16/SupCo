<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="title_static" inherit_id="web.layout" name="Custom Title">
        <xpath expr="//title"
            position="replace">
            <title t-esc="title or 'Công Lý'" />
        </xpath>
    </template>
    <template id="layout" inherit_id="web.layout" name="Customize Favicon and Title">
        <xpath expr="//link" position="replace">
            <link type="image/ico" rel="shortcut icon"
                t-att-href="'/customize_favicon_title/static/img/favicon.ico'" />
        </xpath>
    </template>

</odoo>