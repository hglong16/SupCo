<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="title_static" inherit_id="web.layout" name="Custom Title">
        <xpath expr="//title"
            position="replace">
            <title t-esc="title or 'Công Lý'" />
        </xpath>
        <xpath expr="//head//link"
            position="after">
            <link rel="shortcut icon" href="https://cly.1cdn.vn/assets/congly/images/favicon.ico"></link>
        </xpath>
    </template>
</odoo>